/* pages/mygarage/mygarage.wxss */

.page {
  padding-top: 10px;
}

.car_leiList {
  border-bottom: 10px solid #eee;
  border-left: 10px solid #eee;
  background: #fff;
  border-right: 10px solid #eee;
}
.car_leiList view:first-child {
  border-bottom: 1px solid #eee;
  align-items: center;
}
.car_leiList view:last-child text {
  width: 70px;
  height: 35px;
  line-height: 35px;
  margin-top: 7px;
  text-align: center;
  border-radius: 5px;
  display: inline-block;
  color: #f90;
  box-sizing: border-box;
}

.car_leiList view:last-child {
  justify-content: flex-end;
  position: relative;
  font-size: 12px;
  height: 50px;
  overflow: hidden;
}

.icon-xuanzhong {
  color: #2CA43A;
  display: inline-block;
  width: 54px;
  text-align: center;
}

.carName {
  color: #2CA43A;
  padding: 15px 0;
  width: 98%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  line-height: 20px;
  display: inline-block;
}
